{
    // Processor lists will run a processor for every block placed by the nbt file.
    // The resultant block of the processor list is the block that actually gets placed in the world.
  "processors": [
      // A single Rule processor for this list.
    {
      "processor_type": "minecraft:rule",

      // The rules for this Rule processor to use
      "rules": [
        {
            // The block to look for from the nbt file
            // This has a 50% chance of allowing the found stone bricks block to be replaced
          "input_predicate": {
            "block": "minecraft:stone_bricks",
            "probability": 0.1,
            "predicate_type": "minecraft:random_block_match"
          },
            // This is always true
            // Village roads have this part look for water so roads can be replaced with oak planks instead
          "location_predicate": {
            "predicate_type": "minecraft:always_true"
          },
            // The actual block to place if the above conditions return true
          "output_state": {
            "Name": "minecraft:stone"
          }
        },
        {
          "input_predicate": {
            "block": "minecraft:stone_bricks",
            "probability": 0.1,
            "predicate_type": "minecraft:random_block_match"
          },
          "location_predicate": {
            "predicate_type": "minecraft:always_true"
          },
          "output_state": {
            "Name": "minecraft:gravel"
          }
        },
        {
          "input_predicate": {
            "block": "minecraft:stone_bricks",
            "probability": 0.1,
            "predicate_type": "minecraft:random_block_match"
          },
          "location_predicate": {
            "predicate_type": "minecraft:always_true"
          },
          "output_state": {
            "Name": "minecraft:mossy_stone_bricks",
            "Properties": {
              "lit": false
            }
          }
        },
        {
          "input_predicate": {
            "block": "minecraft:stone_bricks",
            "probability": 0.1,
            "predicate_type": "minecraft:random_block_match"
          },
          "location_predicate": {
            "predicate_type": "minecraft:always_true"
          },

          "output_state": {
            "Name": "minecraft:mossy_cobblestone",
            "Properties": {
              "lit": false
            }
          }
        },
        {
          "input_predicate": {
            "block": "minecraft:stone_bricks",
            "probability": 0.1,
            "predicate_type": "minecraft:random_block_match"
          },
          "location_predicate": {
            "predicate_type": "minecraft:always_true"
          },

          "output_state": {
            "Name": "minecraft:andesite",
            "Properties": {
              "lit": false
            }
          }
        },
        {
          "input_predicate": {
            "block": "minecraft:stone_bricks",
            "probability": 0.1,
            "predicate_type": "minecraft:random_block_match"
          },
          "location_predicate": {
            "predicate_type": "minecraft:always_true"
          },

          "output_state": {
            "Name": "minecraft:cobblestone",
            "Properties": {
              "lit": false
            }
          }
        },
        {
          "input_predicate": {
            "block": "minecraft:stone_bricks",
            "probability": 0.01,
            "predicate_type": "minecraft:random_block_match"
          },
          "location_predicate": {
            "predicate_type": "minecraft:always_true"
          },

          "output_state": {
            "Name": "minecraft:tuff",
            "Properties": {
              "lit": false
            }
          }
        },
        {
          "input_predicate": {
            "block": "minecraft:stone_bricks",
            "probability": 0.2,
            "predicate_type": "minecraft:random_block_match"
          },
          "location_predicate": {
            "predicate_type": "minecraft:always_true"
          },

          "output_state": {
            "Name": "minecraft:infested_cracked_stone_bricks",
            "Properties": {
              "lit": false
            }
          }
        }
      ]
    }
  ]
}